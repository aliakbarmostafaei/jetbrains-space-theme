{
  "name": "Space Light",
  "dark": false,
  "author": "Ali Akbar Mostafāëi",
  "editorScheme": "/colorSchemes/light/space_light.xml",
  "colors": {
    "alphaOverlay": "#ffffff30",
    "paperWhite": "#fcfcfc",
    "floralWhite": "#fffefe",
    "seashell": "#fdf3eb",
    "blackPearl": "#1a1c28",
    "charcoal": "#3c3f54",
    "shuttle": "#5c6166",
    "aluminium": "#aeafb3",
    "mist": "#cdcfd0",
    "pineapple": "#52350B",
    "sawtooth": "#ed9366",
    "casablanca": "#f2ae49",
    "sunset": "#fa8d3e",
    "sunshade": "#ffaa33",
    "caramel": "#ffd192",
    "sands": "#fedeb2",
    "emerald": "#5dd57b",
    "apple": "#6cbf43",
    "pistachio": "#97d27b",
    "oceanGreen": "#4cbf99",
    "oceanBlue": "#3c73ab",
    "electricBlue": "#22a4e6",
    "fountainBlue": "#55b4d4",
    "cobaltBlue": "#7daddb",
    "skyBlue": "#a9d9e9",
    "riverBlue": "#d9e9f8",
    "aliceBlue": "#edf4fa",
    "lavendar": "#a37acc",
    "lilac": "#c594c5",
    "flamingo": "#e65050",
    "begonia": "#f07171",
    "blush": "#ff7383",
    "cottonCandy": "#ff9ca8"
  },
  "icons": {
    "/actions/more.svg": "/icons/light/more_dark.svg"
  },
  "ui": {
    "*": {
      "background": "floralWhite",
      "foreground": "shuttle",
      "borderColor": "floralWhite",
      "selectionBackground": "riverBlue",
      "selectionForeground": "shuttle",
      "disabledForeground": "mist",
      "disabledText": "mist",
      "hoverBackground": "aliceBlue",
      "hoverBorderColor": "aliceBlue"
    },
    "Borders": {
      "color": "floralWhite",
      "ContrastBorderColor": "mist"
    },
    "Window": {
      "border": "floralWhite",
      "undecorated.border": "floralWhite"
    },
    "windowBorder": "floralWhite",
    "Editor": {
      "background": "floralWhite",
      "foreground": "shuttle"
    },
    "Menu": {
      "acceleratorForeground": "shuttle",
      "acceleratorSelectionForeground": "shuttle",
      "Selection": {
        "arc": 4.0
      }
    },
    "MenuItem": {
      "acceleratorForeground": "shuttle",
      "acceleratorSelectionForeground": "shuttle"
    },
    "CompletionPopup": {
      "matchForeground": "oceanBlue",
      "selectionBackground": "riverBlue",
      "selectionInactiveBackground": "aliceBlue"
    },
    "Component": {
      "errorFocusColor": "flamingo",
      "borderColor": "mist",
      "focusColor": "sunshade",
      "focusedBorderColor": "floralWhite",
      "focusWidth": 1.0
    },
    "Plugins": {
      "Button": {
        "installForeground": "pineapple",
        "installFillForeground": "pineapple",
        "installBorderColor": "sunshade",
        "installFillBackground": "sunshade",
        "installFocusedBackground": "sands",
        "updateBackground": "sands",
        "updateBorderColor": "sands",
        "updateForeground": "pineapple"
      }
    },
    "CheckBox": {
      "select": "sunshade"
    },
    "Counter": {
      "background": "sunshade",
      "foreground": "floralWhite"
    },
    "IconBadge": {
      "infoBackground": "sunshade",
      "successBackground": "apple",
      "warningBackground": "sunset",
      "errorBackground": "flamingo"
    },
    "MemoryIndicator": {
      "allocatedBackground": "sands",
      "usedBackground": "caramel"
    },
    "ComboBox": {
      "ArrowButton.iconColor": "sunset",
      "ArrowButton.disabledIconColor": "sands"
    },
    "Button": {
      "foreground": "pineapple",
      "startBackground": "floralWhite",
      "endBackground": "floralWhite",
      "startBorderColor": "sunshade",
      "endBorderColor": "sunshade",
      "focusedBorderColor": "sunshade",
      "shadowWidth": 0,
      "default": {
        "foreground": "pineapple",
        "startBackground": "sunshade",
        "endBackground": "sunshade",
        "startBorderColor": "sunshade",
        "endBorderColor": "sunshade",
        "focusedBorderColor": "sunshade",
        "shadowColor": "floralWhite"
      }
    },
    "ActionButton": {
      "pressedBackground": "sands",
      "pressedBorderColor": "sands"
    },
    "EditorTabs": {
      "background": "floralWhite",
      "underlinedTabBackground": "floralWhite",
      "underlineColor": "sunshade",
      "inactiveUnderlineColor": "caramel",
      "hoverBackground": "seashell",
      "inactiveColoredFileBackground": "seashell"
    },
    "DefaultTabs": {
      "background": "floralWhite",
      "underlinedTabBackground": "floralWhite",
      "underlineColor": "sunshade",
      "inactiveUnderlineColor": "caramel",
      "hoverBackground": "seashell"
    },
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "floralWhite",
        "borderColor": "mist"
      },
      "Button": {
        "selectedBackground": "caramel",
        "selectedForeground": "shuttle"
      }
    },
    "RunWidget": {
      "runIconColor": "oceanGreen",
      "runningBackground": "oceanGreen",
      "stopBackground": "blush",
      "hoverBackground": "alphaOverlay"
    },
    "Notification": {
      "background": "seashell",
      "borderColor": "sunshade",
      "errorBorderColor": "flamingo",
      "arc": 4.0,
      "Button": {
        "background": "sunshade",
        "foreground": "pineapple"
      }
    },
    "Debugger": {
      "EvaluateExpression.background": "floralWhite"
    }
  }
}
